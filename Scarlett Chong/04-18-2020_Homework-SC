/* 4. Recreate the following array by defining a Book constructor and creating 3 book objects from it:
var library = [
   {
       author: 'Bill Gates',
       title: 'The Road Ahead',
       readingStatus: true
   },
   {
       author: 'Steve Jobs',
       title: 'Walter Isaacson',
       readingStatus: true
   },
   {
       author: 'Suzanne Collins',
       title:  'Mockingjay: The Final Book of The Hunger Games',
       readingStatus: false
   }];
*/

//My new code
function Book1(author, title, readingStatus){
  this.author = 'Bill Gates';
  this.title = 'The Road Ahead';
  this.readingStatus = true;
}

function Book2(author, title, readingStatus){
  this.author = 'Steve Jobs';
  this.title = 'Walter Isaacson';
  this.readingStatus = true;
}

function Book3(author, title, readingStatus){
  this.author = 'Suzanne Collins';
  this.title = 'Mockingjay: The Final Book of The Hunger Games';
  this.readingStatus = false;
}
